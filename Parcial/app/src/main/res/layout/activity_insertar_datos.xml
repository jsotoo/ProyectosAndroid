<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jonathan.parcial.InsertarDatos">

    <EditText
        android:id="@+id/txtCargo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:background="#fad60a"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="221dp"
        android:layout_below="@+id/txtNombres"
        android:layout_alignLeft="@+id/txtNombres"
        android:layout_alignStart="@+id/txtNombres"
        android:layout_marginTop="80dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Empresa"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="273dp"
        android:layout_below="@+id/txtCargo"
        android:layout_alignLeft="@+id/txtCargo"
        android:layout_alignStart="@+id/txtCargo"
        android:layout_marginTop="32dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/txtCargo"
        android:layout_alignLeft="@+id/txtNombres"
        android:layout_alignStart="@+id/txtNombres"
        android:layout_marginBottom="34dp"
        android:text="Nombres"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="69dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Insertar Empleados"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="134dp"
        tools:layout_editor_absoluteY="25dp"
        android:layout_marginTop="19dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView3"
        android:layout_toEndOf="@+id/textView3"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/txtCargo"
        android:layout_alignLeft="@+id/txtCargo"
        android:layout_alignStart="@+id/txtCargo"
        android:layout_marginBottom="38dp"
        android:text="Cargo"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="186dp" />

    <EditText
        android:id="@+id/txtNombres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtEmpresa"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/txtEmpresa"
        android:layout_marginTop="110dp"
        android:width="200dp"
        android:background="#fad60a"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="110dp" />

    <EditText
        android:id="@+id/txtEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:background="#fad60a"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="329dp"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="16dp" />

    <Button
        android:id="@+id/btnInsertar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INSERTAR"
        tools:layout_editor_absoluteX="134dp"
        tools:layout_editor_absoluteY="399dp"
        android:layout_marginLeft="66dp"
        android:layout_marginStart="66dp"
        android:layout_alignTop="@+id/txtEmpresa"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4"
        android:layout_marginTop="77dp" />
</RelativeLayout>
